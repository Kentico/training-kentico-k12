<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>WebServices</CodeName>
    <GUID>06c6a5b2-cc99-4f58-81f3-c5f9ca63bcac</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Displays data retrieved using the Kentico REST Service in a table.]]>
  </WebPartDescription>
  <WebPartDisplayName>Grid for REST Service</WebPartDisplayName>
  <WebPartDocumentation>
    <![CDATA[This web part obtains XML data from the system by querying the Kentico REST Service using a specified URL and then displays it in a grid (table). Basic authentication is used to connect to the service.]]>
  </WebPartDocumentation>
  <WebPartFileName>WebServices/GridForRESTService.ascx</WebPartFileName>
  <WebPartGUID>55a23728-c8ff-4a9a-b49b-a2a94c4d7b11</WebPartGUID>
  <WebPartIconClass>icon-w-grid-for-rest-service</WebPartIconClass>
  <WebPartName>GridForRESTService</WebPartName>
  <WebPartProperties>
    <form version="2">
      <field column="RESTServiceQueryURL" columnsize="500" columntype="text" guid="fe093ec6-549c-405f-9bc1-de2b08ced14a" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>REST Service query URL</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the URL of the request that will be used to query the REST Service and obtain the data in XML format.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>True</Trim>
        </settings>
      </field>
      <field allowempty="true" column="UserName" columnsize="100" columntype="text" guid="db7d180f-39fd-4d44-91a3-a2d22e6d46c3" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Username</fieldcaption>
          <fielddescription>
            <![CDATA[Username to use for basic authentication. If the service does not require authentication, leave this field empty.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>True</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Password" columnsize="100" columntype="text" guid="b8742cdf-fa46-4bf7-9056-2181ebea37d1" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Password</fieldcaption>
          <fielddescription>User password for basic authentication.</fielddescription>
        </properties>
        <settings>
          <controlname>formpassword</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>