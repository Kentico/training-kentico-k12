  <WebPartDescription>
    <![CDATA[Displays a filter that can be used to limit which pages should be displayed from a data source. The pages are filtered according to their name. It is intended for use in combination with a Pages data source web part.]]>
  </WebPartDescription>
  <WebPartDisplayName>Page name filter</WebPartDisplayName>
  <WebPartFileName>Filters/DocumentNameFilter.ascx</WebPartFileName>
  <WebPartGUID>8278082c-c2b5-4677-a08b-810383c01239</WebPartGUID>
  <WebPartIconClass>icon-w-document-name-filter</WebPartIconClass>
  <WebPartName>DocumentNameFilter</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Filter properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="ButtonText" columnsize="200" columntype="text" guid="98c037f2-f570-407a-8557-54d08db3ca3e" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Search</defaultvalue>
          <fieldcaption>Button text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed as the caption of the button that applies the filter to the data.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ButtonCssClass" columnsize="200" columntype="text" guid="3ced9dd3-e569-411a-8c3e-3be5c9b1133a" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Button css class</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the name of the CSS class from the used stylesheet that will be applied to the filter button.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
