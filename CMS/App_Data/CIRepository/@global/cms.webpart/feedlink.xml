<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Syndication</CodeName>
    <GUID>9ed02567-2d52-4db2-820f-6b3070e4724f</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDescription>
    <![CDATA[The Feed link web part displays a link to RSS, Atom or XML feed based on the web part settings. It supports automatic feed discovery.]]>
  </WebPartDescription>
  <WebPartDisplayName>Feed link</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Syndication/FeedLink.ascx</WebPartFileName>
  <WebPartGUID>0f9412d2-15e2-4ac5-be78-83181055c04b</WebPartGUID>
  <WebPartIconClass>icon-w-feed-link</WebPartIconClass>
  <WebPartName>FeedLink</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Feed link">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="LinkText" columnsize="200" columntype="text" guid="a526fda9-647b-4edd-8947-f288965a9d2d" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Feed</defaultvalue>
          <fieldcaption>Link text</fieldcaption>
          <fielddescription>Text for the feed link.</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="LinkIcon" columnsize="2000" columntype="text" guid="2495bd2a-71c5-4599-a15a-d05ddd6dc620" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Link icon</fieldcaption>
          <fielddescription>Icon which will be displayed in the feed link.</fielddescription>
        </properties>
        <settings>
          <controlname>iconselector</controlname>
        </settings>
      </field>
      <field column="FeedURL" columnsize="2000" columntype="text" guid="7dd88605-7f7c-49c1-b420-612c34280d96" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Feed URL</fieldcaption>
          <fielddescription>URL of the feed.</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field column="EnableAutodiscovery" columntype="boolean" guid="2d9639d7-ce1f-4be7-8ac3-3a9eb420d8e0" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Enable auto-discovery</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the feed is automatically discovered by the browser. The Content type property has to be set accordingly for proper functionality.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="RSS">
        <properties>
          <caption>Feed</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="FeedTitle" columnsize="450" columntype="text" guid="02ea6032-e5bc-4ad6-b171-8cf3700bb520" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Feed title</fieldcaption>
          <fielddescription>URL title of the feed.</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ContentType" columnsize="20" columntype="text" guid="58283330-ffe2-47d9-af38-ecb5cafa574b" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>application/rss+xml</defaultvalue>
          <fieldcaption>Content type</fieldcaption>
          <fielddescription>Type of content to enable feed autodiscovery.</fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <options>
            <![CDATA[<item value="application/rss+xml" text="RSS" /><item value="application/atom+xml" text="Atom" /><item value="application/xml" text="XML" />]]>
          </options>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>5</WebPartType>
</cms.webpart>