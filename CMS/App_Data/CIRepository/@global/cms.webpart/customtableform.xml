<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>CustomTables</CodeName>
    <GUID>cdadf49c-ed8b-4679-87e1-9cf857b4b121</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Allows you to insert and edit custom table items directly on the page.]]>
  </WebPartDescription>
  <WebPartDisplayName>Custom table form</WebPartDisplayName>
  <WebPartFileName>CustomTables/CustomTableForm.ascx</WebPartFileName>
  <WebPartGUID>54024117-a1e9-4f1f-b97d-0029c5093120</WebPartGUID>
  <WebPartIconClass>icon-w-custom-table-form</WebPartIconClass>
  <WebPartName>CustomTableForm</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="CustomTableFormSettings">
        <properties>
          <caption>Custom table form settings</caption>
          <visible>true</visible>
        </properties>
      </category>
      <field column="CustomTable" columnsize="200" columntype="text" guid="f0fae259-ef94-4d91-907c-fe19bcb95796" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Custom table</fieldcaption>
          <fielddescription>
            <![CDATA[Name of the Custom table form that should be edited.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>customtableselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ItemKeyName" columnsize="200" columntype="text" guid="30502fcc-23dc-417d-a818-d64e85420ad9" publicfield="false" visible="true">
        <properties>
          <defaultvalue>ItemID</defaultvalue>
          <fieldcaption>Item key name</fieldcaption>
          <fielddescription>Key name used to identify the object in edit form.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="AlternativeFormName" columnsize="200" columntype="text" guid="90e51f82-333f-4721-b978-943e5a35fb1f" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Alternative form name</fieldcaption>
          <fielddescription>
            <![CDATA[Alternative form full name (ClassName.AlternativeFormName). Alternative form definition and layout are used if specified.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>selectalternativeform</controlname>
        </settings>
      </field>
      <field allowempty="true" column="UseColonBehindLabel" columntype="boolean" guid="b1d4043a-96da-4675-8831-4397640b5dd9" publicfield="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use colon (:) in labels</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the colon (:) should be used behind the label text.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ValidationErrorMessage" columnsize="250" columntype="text" guid="8b387575-fd2b-451b-ae88-4cb8ca24323f" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Validation error message</fieldcaption>
          <fielddescription>
            <![CDATA[This message will be displayed after validation failed. Default error message will be used if this one is not set.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <category name="ConversionTracking">
        <properties>
          <caption>Conversion tracking</caption>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="TrackConversionName" columnsize="400" columntype="text" guid="cd451b5c-0321-448d-8b42-45541aebd4be" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Track conversion name</fieldcaption>
          <fielddescription>
            <![CDATA[Selects the conversion that will be logged when a user successfully submits the displayed form.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>conversionselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ConversionValue" columntype="double" guid="a1c28dcc-9b70-4245-be05-b9ddca8efe19" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Conversion value</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.conversionvalue$}]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>