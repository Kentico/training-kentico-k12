<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Reporting</CodeName>
    <GUID>a097d3c9-6a18-4f54-b407-4c7b742033ad</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDescription>
    <![CDATA[Can be used to display a report on the pages of the website. Only existing reports created using the Reporting module can be selected.]]>
  </WebPartDescription>
  <WebPartDisplayName>Report</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Reporting/report.ascx</WebPartFileName>
  <WebPartGUID>29db550f-3572-4442-a1c8-4f5dd63fea1c</WebPartGUID>
  <WebPartIconClass>icon-w-report</WebPartIconClass>
  <WebPartName>Report</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Report properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="ReportName" columnsize="100" columntype="text" guid="b4537b71-e59f-406c-8a84-61af8849aa92" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Report name</fieldcaption>
          <fielddescription>
            <![CDATA[Allows the selection of the report to be displayed. If the specified report has some parameters defined, their values can be entered here using the Set parameters button. However, if the Display filter property is enabled, these settings will be ignored, since the parameters will be configurable on the live site using the filter.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>reportselectordropdown</controlname>
        </settings>
      </field>
      <field column="DisplayFilter" columntype="boolean" guid="92278a41-3689-40d2-8599-d187f7d7a1e9" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Display filter</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the parameter filter should be displayed above the report (if the report has some parameters specified).]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="EnableExport" columntype="boolean" guid="68bff69f-8337-4b0c-a0b0-37ac2c679345" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Enable export</fieldcaption>
          <fielddescription>
            <![CDATA[{$If enabled, users will be able to export the data displayed in the report's charts and tables to other formats (Excel, CSV or XML). The export feature may be accessed through a context menu that can be opened by right clicking the rendered chart/table. Please note that data export may be disabled for individual charts/tables via their properties in the main Reporting interface.$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="EnableSubscription" columntype="boolean" guid="6e0c5d82-d1ea-4652-ac7b-baa9bba9862c" publicfield="false" resolvedefaultvalue="False" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>{$rep.enablesubscription$}</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether authorized users should be allowed to subscribe to the components (graphs, tables or values) in the displayed report. Subscription can be done by right clicking on the appropriate item and selecting the 'Subscribe to' option in the context menu. In addition to this property, subscription also needs to be enabled for the given report and the specific graph, table or value in the main reporting interface.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>CheckBoxControl</controlname>
        </settings>
      </field>
      <category name="System settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="PartialCacheMinutes" columnsize="200" columntype="text" guid="1d83edd9-bc28-4f6e-a563-0533301496d8" publicfield="false" visibility="none">
        <settings>
          <controlname>labelcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="PartialCacheDependencies" columnsize="200" columntype="text" guid="f6ba9295-40d2-4b30-b421-8bd96debe905" publicfield="false" visibility="none">
        <settings>
          <controlname>labelcontrol</controlname>
        </settings>
      </field>
      <category name="Hiden data properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="ParametersXmlSchema" columntype="longtext" guid="84af03a5-81f1-41b9-ae82-c455ff56035a" publicfield="false" spellcheck="false" visibility="none">
        <settings>
          <controlname>labelcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ParametersXmlData" columntype="longtext" guid="837746e7-96ef-4bb4-bb4d-96a3550806f3" publicfield="false" spellcheck="false" visibility="none">
        <settings>
          <controlname>labelcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Reporting</CodeName>
    <GUID>6620e618-9925-4297-bcf7-ed6d9e21ba21</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>