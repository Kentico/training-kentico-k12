<cms.webpart>
  <WebPartCategoryID>
    <CodeName>MembershipLiveID</CodeName>
    <GUID>6890dd98-65c5-4c32-8261-e2dfc9f4ae77</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
