<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Text</CodeName>
    <GUID>68d8e65b-a01c-49ae-b9b6-7580bbce4b09</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDescription>
    <![CDATA[This web part highlights all words or characters on the page matching the specified text or regular expression. Highlighting is done using a selected CSS class.]]>
  </WebPartDescription>
  <WebPartDisplayName>Text highlighter</WebPartDisplayName>
  <WebPartFileName>General/TextHighlighter.ascx</WebPartFileName>
  <WebPartGUID>206ebecb-bcc2-4148-9c7e-76ca796141ac</WebPartGUID>
  <WebPartIconClass>icon-w-text-highlighter</WebPartIconClass>
  <WebPartName>TexHighlighter</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Text highlight">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="CSSClassName" columnsize="50" columntype="text" guid="7e0fa47d-d61b-479d-af33-d5c6f0648541" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>highlight</defaultvalue>
          <fieldcaption>CSS class name</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the name of the CSS class (from the currently used CSS stylesheet) that will be applied to the span element into which highlighted words will be wrapped.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="TextToHighlight" columnsize="100" columntype="text" guid="71ac283a-9e2e-4ee6-894d-fcb5c2a3a74e" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Text to highlight</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the character, word or phrase that should be highlighted. Alternatively, a standard regular expression may be entered (only if the Text is regular expression property is enabled). Please note that this is case sensitive.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="TextIsRegularExpression" columntype="boolean" guid="acfc5719-292a-4b82-87bb-5478e300955d" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Text is regular expression</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the value of the Text to highlight property will be resolved as a regular expression.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>4</WebPartType>
</cms.webpart>