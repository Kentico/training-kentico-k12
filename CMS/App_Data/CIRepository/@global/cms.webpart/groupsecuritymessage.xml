<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>GroupSecurity</CodeName>
    <GUID>3be83bc1-a16c-4bfb-b105-1c1c2c7e7ccc</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultConfiguration>
    <page />
  </WebPartDefaultConfiguration>
  <WebPartDescription>
    <![CDATA[Displays a message to users who do not have sufficient permissions to view the content of the currently active group. A different message can be displayed to different types of users.]]>
  </WebPartDescription>
  <WebPartDisplayName>Group security message</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Community/Profile/GroupSecurityMessage.ascx</WebPartFileName>
  <WebPartGUID>1779d24f-6d63-42c5-ba44-824e5ffbcf4b</WebPartGUID>
  <WebPartIconClass>icon-w-group-security-message</WebPartIconClass>
  <WebPartName>GroupSecurityMessage</WebPartName>
  <WebPartProperties>
    <form version="2">
      <field allowempty="true" column="LoginURL" columnsize="500" columntype="text" guid="932aab4e-6593-4b92-9cb8-7baef693c593" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Login URL</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.loginurl$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="JoinGroupPath" columnsize="500" columntype="text" guid="04cd840a-6375-40b3-9646-a214f902255a" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Join group path</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.joingroupurl$}</fielddescription>
        </properties>
        <settings>
          <AllowSetPermissions>False</AllowSetPermissions>
          <controlname>selectsinglepath</controlname>
        </settings>
      </field>
      <category name="Messages">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SiteMembersOnlyText" columntype="longtext" guid="38889eec-768d-4583-adf8-3681ae3b74e0" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Site members only text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text which will be displayed to public (unauthenticated) users if the pages of the group are only allowed for user who are registered on the website. You can enter text between the {0} and {1} macro expressions to add a link to the website's sign in page.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <field allowempty="true" column="GroupMembersOnlyText" columntype="longtext" guid="1ee8ebe8-fc03-442a-88e1-61895fbd97bd" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Group members only text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text which will be displayed to public (unauthenticated) users if the content of the current group is only available for its members. You can enter text between the {0} and {1} macro expressions to add a link to the website's sign in page.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <field allowempty="true" column="GroupMembersOnlyAuthenticatedText" columntype="longtext" guid="1fd1f368-3937-4307-869d-0a6b792f02fd" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Group members only text (site member)</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text which will be displayed to authenticated users if the content of the current group is only available for its members. You can enter text between the {0} and {1} macro expressions to add a link that allows users to submit a request to join the current group.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Community</CodeName>
    <GUID>f1cc54a9-d5bb-4a69-bd8c-8918eb410656</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>